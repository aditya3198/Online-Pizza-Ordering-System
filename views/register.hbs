<header class="w3-container w3-teal" id="reg">
    <h1>Register</h1>
</header>
<form class="px-4 py-3" class="form-horizontal" id="regestrationform">
    <div class="form-group" class="form-horizontal">
        <label for="name" class="control-label">Name:</label>
        <input type="text" class="form-control" id="name" placeholder="Abhi Roy" required />
    </div>
    <div class="form-group" class="form-horizontal">
        <label for="number" class="control-label">Mobile number:</label>
        <input type="number" class="form-control" id="number" placeholder="9876543210" />
    </div>
    <div class="form-group" class="form-horizontal">
        <label for="email" class="control-label">Email address</label>
        <input type="email" class="form-control" id="email" placeholder="email@example.com" required />
    </div>
    <div class="form-group" class="form-horizontal">
        <label for="password" class="control-label">Password</label>
        <input type="password" class="form-control" id="password" placeholder="Password" required onkeyup="check()" />
    </div>
    <div class="form-group" class="form-horizontal">
        <label for="confirm_password" class="control-label">Confirm Password</label>
        <input type="password" class="form-control" id="confirm_password" placeholder="Confirm Password" required
            onkeyup="check()" />
        <span id="message"></span>
    </div>
    <button type="submit" class="btn btn-primary">Register</button>
</form>